part
{
	dec {
		tmpl cpp/declarations.jinja
		file "${mod}_${mod_a}_${mod_i}/declarations.hpp"
		naming underscore
	}
	header {
		inc_part dec
		tmpl cpp/header.jinja
		file "${mod}_${mod_a}_${mod_i}/module.hpp"
		naming underscore
	}
	server_header {
		inc_part header
		tmpl cpp/nghttp_srv_hdr.jinja
		file "${mod}_${mod_a}_${mod_i}/server.hpp"
		naming underscore
	}
	server {
		inc_part server_header
		tmpl cpp/nghttp_srv.jinja
		file "${mod}_${mod_a}_${mod_i}/server.cpp"
		naming underscore
	}
}

